import java.util.Scanner;

public class Solution {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();

        for ( int i = 0; i < n; i ++ ) {
            int cnt = 0;
            String str = "";

            while ( true ) {
                str += "*";
                if ( i == cnt ) {
                    System.out.println(str);
                    break;
                }
                cnt ++;
            }
        }
    }
}
