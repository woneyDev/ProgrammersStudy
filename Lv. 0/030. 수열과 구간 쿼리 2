class Solution {
    public int[] solution(int[] arr, int[][] queries) {
        int[] answer = new int[queries.length];

        for ( int i = 0; i < queries.length; i ++ ) {
            int cnt = 0;
            int temp = 0;
            for ( int j = queries[i][0]; j <= queries[i][1]; j ++ ) {
                if ( arr[j] > queries[i][2] ) {
                    cnt ++;
                    if ( cnt == 1 ) {
                        temp = arr[j];
                    }else {
                        if ( temp > arr[j] ) {
                            temp = arr[j];
                        }
                    }
                }else {
                    if ( cnt == 0 ) {
                        temp = -1;
                    }
                }
            }
            answer[i] = temp;
        }
        return answer;
    }
}
