class Solution {
    public int solution(int a, int b) {
        int answer = 0;
        int temp = 0;
        String str = String.valueOf(a) + String.valueOf(b);
        temp = 2 * a * b;

        if ( Integer.parseInt(str) > temp ) {
            answer = Integer.parseInt(str);
        }else{
            answer = temp;
        }
        return answer;
    }
}
