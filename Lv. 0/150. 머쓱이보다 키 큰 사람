class Solution {
    public int solution(int[] array, int height) {
        int result = 0;
        
        int[] hgtArr = array;
        int hgt = height;

        for ( int i = 0; i < hgtArr.length; i ++ ) {
            if ( hgtArr[i] > hgt ) {
                result ++;
            }
        }
        return result;
    }
}
