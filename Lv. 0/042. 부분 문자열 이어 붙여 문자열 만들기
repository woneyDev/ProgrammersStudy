class Solution {
    public String solution(String[] my_strings, int[][] parts) {
        String answer = "";
        int j = 0;

        for ( int i = 0; i < parts.length; i ++ ) {
            answer += my_strings[i].substring(parts[i][j], parts[i][j + 1] + 1);
        }

        return answer;
    }
}
