import java.util.ArrayList;

class Solution {
    public int[] solution(int n) {
        ArrayList<Integer> tmp = new ArrayList<>();
        for ( int i = 1; i <= n; i ++ ) {
            if ( i % 2 == 1 ) {
                tmp.add(i);
            }
        }
        int[] answer = new int[tmp.size()];
        for ( int j = 0; j < tmp.size(); j ++ ) {
            answer[j] = tmp.get(j);
        }
        return answer;
    }
}
