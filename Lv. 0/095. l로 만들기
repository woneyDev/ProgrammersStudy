class Solution {
    public String solution(String myString) {
        String answer = "";
        char l = 'l';
        
        for ( int i = 0; i < myString.length(); i ++ ) {
            if ( (int)((char)myString.charAt(i)) < (int)l ) {
                answer += "l";
            }else {
                answer += String.valueOf(myString.charAt(i));
            }
        }
        
        return answer;
    }
}
