class Solution {
    public long solution(String numbers) {
        long answer = 0;
        String tmp = "";
        String result = "";
        for ( int i = 0; i < numbers.length(); i ++ ) {
            tmp += numbers.charAt(i);
            switch ( tmp ) {
                case "one"      : tmp = ""; result += "1"; break;
                case "two"      : tmp = ""; result += "2"; break;
                case "three"    : tmp = ""; result += "3"; break;
                case "four"     : tmp = ""; result += "4"; break;
                case "five"     : tmp = ""; result += "5"; break;
                case "six"      : tmp = ""; result += "6"; break;
                case "seven"    : tmp = ""; result += "7"; break;
                case "eight"    : tmp = ""; result += "8"; break;
                case "nine"     : tmp = ""; result += "9"; break;
                case "zero"     : tmp = ""; result += "0"; break;
            }
        }
        answer = Long.parseLong(result);
        return answer;
    }
}
