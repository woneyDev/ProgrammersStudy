class Solution {
    public boolean solution(String s) {
        if ( s.length() != 4 && s.length() != 6) return false;
        boolean answer = true;
        for ( int i = 0; i < s.length(); i ++ ) {
            if ( (int)s.charAt(i) >= 65 ) {
                answer = false;
                break;
            }
        }
        return answer;
    }
}
