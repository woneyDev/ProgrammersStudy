class Solution {
    public String solution(String[] seoul) {
        int i = 0;
        for ( i = 0; i < seoul.length; i ++ ) {
            if ( "Kim".equals(seoul[i]) ) {
                break;
            }
        }
        
        String answer = "김서방은 " + i + "에 있다";
        return answer;
    }
}
